{
    "data" : {
        "name": "test_metabolomic_binary",
        "data_type": "metabolomic",
        "file_path": "/data/metabolites_drought.csv",
        "save_path": "/experiments/",
        "metadata_file": "/data/metabolites_drought_metadata.csv",
        "file_path_holdout_data": "/data/metabolites_drought.csv",
        "metadata_file_holdout_data": "/data/metabolites_drought_metadata.csv",
        "target": "Drought"
    },
    "ml":{
        "seed_num": 42,
        "test_size": 0.2,
        "problem_type": "classification",
        "hyper_tuning": "random",
        "hyper_budget": 50,
        "stratify_by_groups": "N",
        "groups": "",
        "oversampling": "Y",
        "fit_scorer": "f1",
        "scorer_list": [
            "acc",
            "f1"
        ],
        "model_list": [
            "rf",
            "adaboost",
            "knn",
            "autoxgboost",
            "autolgbm",
            "autosklearn",
            "autokeras"
        ],
        "autosklearn_config": {
            "verbose": true,
            "estimators": [
                "bernoulli_nb",
                "decision_tree",
                "extra_trees",
                "gaussian_nb",
                "k_nearest_neighbors",
                "lda",
                "liblinear_svc",
                "mlp",
                "multinomial_nb",
                "passive_aggressive",
                "qda",
                "random_forest"
            ],
            "time_left_for_this_task": 120,
            "per_run_time_limit": 60,
            "memory_limit": 65536,
            "n_jobs": 1,
            "ensemble_size": 1
        },
        "autokeras_config": {
            "n_epochs": 100,
            "batch_size": 32,
            "verbose": true,
            "n_blocks": 3,
            "dropout": 0.3,
            "use_batchnorm": true,
            "n_trials": 4,
            "tuner": "bayesian"
        },
        "autolgbm_config": {
            "verbose": true,
            "n_trials": 15,
            "timeout": 60
        },
        "autoxgboost_config": {
            "verbose": true,
            "n_trials": 10,
            "timeout": 1000
        },
        "feature_selection": {
            "k": "auto",
            "var_threshold": 0,
            "auto": {
                "min_features": 10,
                "interval": 1
            },
            "method": {
                "name": "SelectKBest",
                "metric": "f_classif"
            }
        }
    },
    "plotting" :{
        "plot_method": [
            "barplot_scorer",
            "boxplot_scorer",
            "conf_matrix",
            "shap_plots",
            "roc_curve"
        ],
        "top_feats_permImp": 15,
        "top_feats_shap": 15,
        "explanations_data": "all"
    },
    "metabolomic" :{
        "filter_metabolomic_sample": 1000000,
        "output_file_met": "/data/metabolites_drought_processed.csv",
        "output_metadata": "/data/metabolites_drought_metadata_processed.csv",
        "filter_measurements": [
            0,
            0
        ]
    }
}