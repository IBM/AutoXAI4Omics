{
    "data":{
        "name": "bloodUreaNitrogen",
        "data_type": "gene_expression",
        "file_path": "/data/Pre-process_GE_L1000_discrete_drug_perturbation_blood_urea_nitrogen.csv",
        "save_path": "/experiments",
        "target": "blood_urea_nitrogen(mg/dl)",
        "metadata_file": "/data/metadata_L1000.csv"
        
    },
    "ml":{
        "seed_num": 42,
        "test_size": 0.2,
        "problem_type": "regression",
        "stratify_by_groups": "N",
        "groups": "",
        "oversampling": "N",
        "hyper_tuning": "random",
        "hyper_budget": 50,
        "fit_scorer": "mean_ae",
        "scorer_list": [
            "rmse",
            "mean_ae",
            "med_ae"
        ],
        "model_list": [
            "rf",
            "autosklearn",
            "autokeras",
            "autolgbm",
            "autoxgboost"
        ],
        "autokeras_config": {
            "n_epochs": 50,
            "batch_size": 32,
            "verbose": true,
            "n_blocks": 3,
            "dropout": 0.3,
            "use_batchnorm": true,
            "n_trials": 3,
            "tuner": "bayesian"
        },
        "autosklearn_config": {
            "verbose": true,
            "estimators": ["adaboost",  "decision_tree", "extra_trees", "gradient_boosting", "k_nearest_neighbors", "sgd"],
            "time_left_for_this_task": 120,
            "per_run_time_limit": 60,
            "memory_limit": 65536,
            "n_jobs": 1,
            "ensemble_size": 1
        },
        "autolgbm_config": {
            "verbose": true,
            "n_trials": 5,
            "timeout": 60
        },
        "autoxgboost_config": {
            "verbose": true,
            "n_trials": 5,
            "timeout": 60
        },
        "feature_selection": {
            "k": "auto"
        }
        
    },
    "plotting":{
        "plot_method": [
            "boxplot_scorer",
            "barplot_scorer",
            "shap_plots"
        ],
        "top_feats_permImp": 15,
        "top_feats_shap": 15,
        "explanations_data": "all"
    },
    "gene_expression":{
        "expression_type": "OTHER",
        "filter_sample": 100,
        "filter_genes": ["0", "0"],
        "output_file_ge": "/data/processed_gene_expression_L1000_drug_perturbation_blood_urea_nitrogen.csv",
        "output_metadata": "/data/processed_gene_expression_L1000_drug_perturbation_blood_urea_nitrogen_filter.csv",
        
    }
}
